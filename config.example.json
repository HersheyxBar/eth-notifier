        "type": "infura",
        "apiKey": "env:INFURA_API_KEY"
      },
      {
        "type": "quicknode",
        "httpUrl": "env:QUICKNODE_HTTP_URL",
        "wsUrl": "env:QUICKNODE_WS_URL"
      },
      {
        "type": "ankr",
        "apiKey": "env:ANKR_API_KEY"
      }
    ],
    "strategy": "priority"
  },
  "contracts": [
    {
      "name": "Example NFT Collection",
      "address": "0x0000000000000000000000000000000000000000",
